<h1>Listing string_variables</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Description</th>
    <th>Value</th>
  </tr>

<% @string_variables.each do |string_variable| %>
  <tr>
    <td><%=h string_variable.title %></td>
    <td><%=h string_variable.description %></td>
    <td><%=h string_variable.value %></td>
    <td><%= link_to 'Show', string_variable %></td>
    <td><%= link_to 'Edit', edit_string_variable_path(string_variable) %></td>
    <td><%= link_to 'Destroy', string_variable, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New string_variable', new_string_variable_path %>