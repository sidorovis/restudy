<h1>Editing wf_attribute</h1>

<% form_for(@wf_attribute) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :content %><br />
    <%= f.text_field :content %>
  </p>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', @wf_attribute %> |
<%= link_to 'Back', wf_attributes_path %>
